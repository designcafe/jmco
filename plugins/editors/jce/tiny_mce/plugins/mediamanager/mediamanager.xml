<?xml version="1.0" ?>
<install version="1.5" type="plugin" plugin="mediamanager">
	<name>Media Manager</name>
	<version>1.5.6.2</version>
	<creationDate>17 November 2010</creationDate>
	<author>Ryan Demmer</author>
	<authorEmail>info@joomlacontenteditor.net</authorEmail>
	<authorUrl>http://www.joomlacontenteditor.net</authorUrl>
	<copyright>Copyright (C) 2006 - 2010 Ryan Demmer. All rights reserved</copyright>
	<license>GNU/GPL Version 2 - http://www.gnu.org/licenses/gpl-2.0.html</license>
	<description><![CDATA[<h3>JCE Media Manager 1.5.6.2</h3><p>Insert and manage media files</p><p>Changelog - <a href="http://www.joomlacontenteditor.net/support/releases/changelog/media-manager" title="Changelog">http://www.joomlacontenteditor.net/support/releases/changelog/media-manager</a></p>]]></description>
	<icon>mediamanager</icon>
	<layout>mediamanager</layout>
	<elements></elements>
	<files>
		<file>editor_plugin.js</file>
		<file>index.html</file>
		<file>mediamanager.php</file>
		<folder>classes</folder>
		<folder>css</folder>
		<folder>img</folder>
		<folder>js</folder>
		<folder>langs</folder>
		<folder>swf</folder>
	</files>
	<params group="standard">
        <param name="mediamanager_dir" type="text" default="" size="50" label="MANAGER PARAM DIRECTORY" description="MANAGER PARAM DIRECTORY DESC"/>
		<param name="mediamanager_max_size" type="text" default="" label="MANAGER PARAM UPLOAD SIZE" description="MANAGER PARAM UPLOAD SIZE DESC"/>
		<param name="mediamanager_extensions" type="textarea" rows="10" cols="50" default="windowsmedia=avi,wmv,wm,asf,asx,wmx,wvx;quicktime=mov,qt,mpg,mp3,mp4,mpeg;flash=swf,flv,xml;shockwave=dcr;real=rm,ra,ram;divx=divx" label="PARAM EXTENSIONS" description="PARAM EXTENSIONS DESC"/>
    	<param name="mediamanager_flvplayer" type="text" size="100" default="flvplayer.swf" label="PARAM FLVPLAYER" description="PARAM FLVPLAYER DESC"/>
        <param name="mediamanager_flvplayer_path" type="text" size="100" default="plugins/editors/jce/tiny_mce/plugins/mediamanager/swf" label="PARAM FLVPLAYER PATH" description="PARAM FLVPLAYER PATH DESC"/>
	</params>
		<params group="defaults">
		<param name="mediamanager_margin_top" type="list" default="default" label="MARGIN TOP" description="MARGIN DESC">
			<option value="default">NOT SET</option>
			<option value="0">0</option>
			<option value="1">1</option>
			<option value="2">2</option>
			<option value="3">3</option>
			<option value="4">4</option>
			<option value="5">5</option>
			<option value="6">6</option>
			<option value="7">7</option>
			<option value="8">8</option>
			<option value="9">9</option>
			<option value="10">10</option>
		</param>
		<param name="mediamanager_margin_right" type="list" default="default" label="MARGIN RIGHT" description="MARGIN DESC">
			<option value="default">NOT SET</option>
			<option value="0">0</option>
			<option value="1">1</option>
			<option value="2">2</option>
			<option value="3">3</option>
			<option value="4">4</option>
			<option value="5">5</option>
			<option value="6">6</option>
			<option value="7">7</option>
			<option value="8">8</option>
			<option value="9">9</option>
			<option value="10">10</option>
		</param>
		<param name="mediamanager_margin_bottom" type="list" default="default" label="MARGIN BOTTOM" description="MARGIN DESC">
			<option value="default">NOT SET</option>
			<option value="0">0</option>
			<option value="1">1</option>
			<option value="2">2</option>
			<option value="3">3</option>
			<option value="4">4</option>
			<option value="5">5</option>
			<option value="6">6</option>
			<option value="7">7</option>
			<option value="8">8</option>
			<option value="9">9</option>
			<option value="10">10</option>
		</param>
		<param name="mediamanager_margin_left" type="list" default="default" label="MARGIN LEFT" description="MARGIN DESC">
			<option value="default">NOT SET</option>
			<option value="0">0</option>
			<option value="1">1</option>
			<option value="2">2</option>
			<option value="3">3</option>
			<option value="4">4</option>
			<option value="5">5</option>
			<option value="6">6</option>
			<option value="7">7</option>
			<option value="8">8</option>
			<option value="9">9</option>
			<option value="10">10</option>
		</param>
        <param name="mediamanager_border" type="list" default="0" label="BORDER ENABLE" description="BORDER ENABLE DESC">
				<option value="0">NO</option>
                <option value="1">YES</option>
		</param>
		<param name="mediamanager_border_width" type="list" default="default" label="BORDER WIDTH" description="BORDER WIDTH DESC">
				<option value="default">NOT SET</option>
				<option value="0">0</option>
				<option value="1">1</option>
				<option value="2">2</option>
				<option value="3">3</option>
				<option value="4">4</option>
				<option value="5">5</option>
				<option value="6">6</option>
				<option value="7">7</option>
				<option value="8">8</option>
				<option value="9">9</option>
				<option value="thin">BORDER THIN</option>
				<option value="medium">BORDER MEDIUM</option>
				<option value="thick">BORDER THICK</option>
		</param>
		<param name="mediamanager_border_style" type="list" default="default" label="BORDER STYLE" description="BORDER STYLE DESC">
				<option value="default">NOT SET</option>
				<option value="none">BORDER NONE</option>
				<option value="solid">BORDER SOLID</option>
				<option value="dashed">BORDER DASHED</option>
				<option value="dotted">BORDER DOTTED</option>
				<option value="double">BORDER DOUBLE</option>
				<option value="groove">BORDER GROOVE</option>
				<option value="inset">BORDER INSET</option>
				<option value="outset">BORDER OUTSET</option>
			    <option value="ridge">BORDER RIDGE</option>
		</param>
		<param name="mediamanager_border_color" type="text" default="#000000" label="BORDER COLOR" description="BORDER COLOR DESC"/>
        <param name="mediamanager_align" type="list" default="default" label="Default Align value" description="Default Align value">
               <option value="default">NOT SET</option>
                <option value="top">ALIGN TOP</option>
                <option value="middle">ALIGN MIDDLE</option>
                <option value="bottom">ALIGN BOTTOM</option>
                <option value="left">ALIGN LEFT</option>
                <option value="right">ALIGN RIGHT</option>
        </param>
	</params>
	<params group="access">
        <param name="mediamanager_upload" type="list" default="1" label="UPLOAD" description="UPLOAD DESC">
            <option value="1">YES</option>
            <option value="0">NO</option>
		</param>
		<param name="mediamanager_upload_conflict" type="list" default="" label="PARAM UPLOAD EXISTS" description="PARAM UPLOAD EXISTS DESC">
			<option value="">NOT SET</option>
			<option value="overwrite">PARAM UPLOAD EXISTS OVERWRITE</option>
			<option value="unique">PARAM UPLOAD EXISTS UNIQUE</option>
			<option value="overwrite|unique">PARAM UPLOAD EXISTS OVERWRITE PARAM UPLOAD EXISTS UNIQUE</option>
        </param>
		<param name="mediamanager_folder_new" type="list" default="1" label="FOLDER CREATE" description="FOLDER CREATE DESC">
            <option value="1">YES</option>
            <option value="0">NO</option>
		</param>
		<param name="mediamanager_folder_delete" type="list" default="1" label="FOLDER DELETE" description="FOLDER DELETE DESC">
            <option value="1">YES</option>
            <option value="0">NO</option>
		</param>
		<param name="mediamanager_folder_rename" type="list" default="1" label="FOLDER RENAME" description="FOLDER RENAME DESC">
            <option value="1">YES</option>
            <option value="0">NO</option>
		</param>
		<param name="mediamanager_file_delete" type="list" default="1" label="FILE DELETE" description="FILE DELETE DESC">
            <option value="1">YES</option>
            <option value="0">NO</option>
		</param>
		<param name="mediamanager_file_rename" type="list" default="1" label="FILE RENAME" description="FILE RENAME DESC">
            <option value="1">YES</option>
            <option value="0">NO</option>
		</param>
		<param name="mediamanager_file_move" type="list" default="1" label="FILE PASTE" description="FILE PASTE DESC">
            <option value="1">YES</option>
            <option value="0">NO</option>
		</param>
	</params>
	<languages>
		<language tag="en-GB">en-GB.com_jce_mediamanager.ini</language>
	</languages>
	<help>
		<topic key="mediamanager.about" title="Help About" />
		<topic key="mediamanager.interface" title="Help Interface" />
		<topic key="mediamanager.advanced" title="Help Advanced" />
		<topic key="mediamanager.media" title="Help Media" />
		<topic key="mediamanager.insert" title="Help Insert" />
	</help>
</install>